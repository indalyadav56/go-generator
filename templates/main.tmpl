{{define "main"}}

package main

import (
	"{{.ServiceName}}/internal/app"
	"fmt"
)

func main() {
	application, err := app.New()
	if err != nil {
		fmt.Printf("Error creating application: %v\n", err)
		return
	}

	if err := application.Run(); err != nil {
		fmt.Printf("Error running application: %v\n", err)
		return
	}

}

{{end}}
